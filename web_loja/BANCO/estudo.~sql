SELECT PT.ROWID, PT.* FROM TB_PRESENTE_FORNECEDOR PT 

SELECT * FROM TB_PRESENTE_MARCA
SELECT * FROM TB_PRESENTE_FORNECEDOR
SELECT * FROM TB_PRESENTE_TIPO
SELECT * FROM TB_PRESENTE_FINALIDADE
SELECT * FROM TB_PRESENTE


SELECT PTI.COD_TIPO, 
       PTI.DESC_TIPO 
  FROM TB_PRESENTE_TIPO PTI;

SELECT PF.COD_FORNECEDOR, 
       PF.NOME_FORNECEDOR       
  FROM TB_PRESENTE_FORNECEDOR PF;

SELECT PM.COD_MARCA, PM.MARCA_DESC FROM TB_PRESENTE_MARCA PM


SELECT PFI.ROWID, PFI.*
       /*PFI.COD_FINALIDADE, 
       PFI.DESC_FINALIDADE, 
       PFI.FINALIDADE_ORIGEM*/
  FROM TB_PRESENTE_FINALIDADE PFI
  
SELECT * FROM TB_PRESENTE_TIPO PTI

SELECT P.PRESENTE_DESC,
       P.PRESENTE_PRECO,
       PF.NOME_FORNECEDOR,
       PM.MARCA_DESC,
       PTI.DESC_TIPO,
       PFI.DESC_FINALIDADE
  FROM TB_PRESENTE_FINALIDADE PFI,
       TB_PRESENTE_TIPO       PTI,
       TB_PRESENTE_MARCA      PM,
       TB_PRESENTE_FORNECEDOR PF,
       TB_PRESENTE            P
 WHERE 1=1
   AND P.COD_PRESENTE = 1001
   AND PF.COD_FORNECEDOR = P.COD_FORNECEDOR
   AND PM.COD_MARCA = P.COD_MARCA
   AND PTI.COD_TIPO = P.COD_TIPO
   AND PFI.COD_FINALIDADE = P.COD_FINALIDADE 
   
SELECT * FROM TB_PRESENTE
--TRUNCATE TABLE TB_PRESENTE    
   
INSERT INTO TB_PRESENTE
    (COD_PRESENTE,
     PRESENTE_DESC,
     COD_TIPO,
     COD_MARCA,
     COD_FINALIDADE,
     PRESENTE_COR,
     PRESENTE_TAMANHO,
     PRESENTE_PRECO,
     COD_FORNECEDOR)
VALUES
    (SQ_PRESENTE.NEXTVAL,
     'WRV LXE TURBO',
     1,
     2,
     2,
     'AZUL',
     'NA',
     124000.00,
     10281);
    
   
   
   
   
   
   
   
   
   
   
   
   
   
